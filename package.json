{
    "type": "module",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "folders": "mkdir -p data && cp -n ./example/* ./data",
        "start": "npm run folders && ts-node index.ts",
        "watch": "nodemon --exec npm start"
    },
    "nodemonConfig": {
        "restartable": "rs",
        "verbose": false,
        "watch": [
            "./data/parse.ts",
            "./data/input.txt"
        ],
        "ext": "*"
    },
    "dependencies": {
        "@swc/core": "^1.3.22",
        "@types/node": "^18.11.10",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1"
    }
}
